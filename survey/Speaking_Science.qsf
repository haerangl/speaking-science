{"SurveyEntry":{"SurveyID":"SV_3BFqxiZ84ipU6SF","SurveyName":"Speaking Science - v2 Port ships air pollution","SurveyDescription":null,"SurveyOwnerID":"UR_20lNWQCb9x8QlkV","SurveyBrandID":"berkeley","DivisionID":"DV_bE4GVCKSvJJ2JYF","SurveyLanguage":"EN","SurveyActiveResponseSet":"RS_1SQbh1muOruNuJf","SurveyStatus":"Active","SurveyStartDate":"0000-00-00 00:00:00","SurveyExpirationDate":"0000-00-00 00:00:00","SurveyCreationDate":"2020-03-15 10:21:57","CreatorID":"UR_20lNWQCb9x8QlkV","LastModified":"2020-03-18 00:04:41","LastAccessed":"0000-00-00 00:00:00","LastActivated":"2020-03-15 14:32:50","Deleted":null},"SurveyElements":[{"SurveyID":"SV_3BFqxiZ84ipU6SF","Element":"BL","PrimaryAttribute":"Survey Blocks","SecondaryAttribute":null,"TertiaryAttribute":null,"Payload":{"0":{"Type":"Default","Description":"Welcome","ID":"BL_cAwR5HlqggAmv3v","BlockElements":[{"Type":"Question","QuestionID":"QID1"},{"Type":"Question","QuestionID":"QID7"}]},"1":{"Type":"Trash","Description":"Trash \/ Unused Questions","ID":"BL_4ODwwn7NfdjWPNX","BlockElements":[],"Options":{"BlockLocking":"false","RandomizeQuestions":"false","BlockVisibility":"Expanded"}},"4":{"Type":"Standard","SubType":"","Description":"Article - New York","ID":"BL_3ZNmZsz6CObnGND","BlockElements":[{"Type":"Question","QuestionID":"QID54"},{"Type":"Question","QuestionID":"QID9"},{"Type":"Question","QuestionID":"QID10"}],"Options":{"BlockLocking":false,"RandomizeQuestions":"false","BlockVisibility":"Expanded"}},"5":{"Type":"Standard","SubType":"","Description":"Quiz","ID":"BL_8kyGy1Nl2Ld6Yhn","BlockElements":[{"Type":"Question","QuestionID":"QID19"},{"Type":"Question","QuestionID":"QID12"},{"Type":"Question","QuestionID":"QID60"},{"Type":"Question","QuestionID":"QID11"},{"Type":"Question","QuestionID":"QID14"},{"Type":"Question","QuestionID":"QID32"},{"Type":"Question","QuestionID":"QID33"},{"Type":"Question","QuestionID":"QID15"},{"Type":"Question","QuestionID":"QID61"},{"Type":"Question","QuestionID":"QID17"}]},"6":{"Typ